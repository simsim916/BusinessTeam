package com.ncs.spring02.domain;

//** DTO
//=> private 맴버변수
//=> getter/setter
//=> toString

public class MemberDTO {
	private String id; // Primary_Key
	private String password; // not null
	private String name;
	private int age;
	private int jno;
	private String info;
	private double point;
	private String birthday;
	private String rid; // 추천인
	
}
